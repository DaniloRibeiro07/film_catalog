<h2>Diretores Cadastrados</h2>
<%= link_to 'Cadastra diretor de um filme', new_director_path %>
<ul>
    <%Director.all.each do |director|%>
    <li>
      <%= link_to director.name, director_path(director.id) %>
      <%= link_to 'Editar', edit_director_path(director.id) %>
    </li>
  <%end%>
</ul>